            <field name="prefix">O%(y)s%(month)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Opportunity -->
        <record id="seq_helpdesk_ticket" model="ir.sequence">
